#include "libft.h"

char	*ft_strnstr(const char *big, const char *little, size_t len);

char	*ft_strnstr(const char *big, const char *little, size_t len)
{
	size_t i, j;
	i = 0;
	if (little[0] == '\0')
		return ((char *)big);
	while (big[i] && i < len)
	{
		j = 0;
		while (big[i+j] != '\0' && big[i+j] == little[j] && j < len)
		{
			if (little[j+1] == '\0')
				return ((char *)&big[i]);
			j++;
		}
		i++;
	}
	return(0);
}

