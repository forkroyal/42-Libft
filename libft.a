#include "libft.h"

void	*ft_memmove(void *dest, const void *src, size_t n);

void	*ft_memmove(void *dest, const void *src, size_t n)
{
	char		*mydest;
	const char	*mysrc;
	int			i;

	if (!dest || !src)
		return (0);
	mydest = dest;
	mysrc = src;
	i = 0;
	if (dest < src)
	{
		while (i++ < n)
			mydest[i] = mysrc[i];
	}
	else
	{
		while (i++ < n)
			mydest[n - i - 1] = mysrc[n - i - 1];
	}
	return (dest);
}

