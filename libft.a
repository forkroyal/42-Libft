char	*ft_strjoin(char const *s1, char const *s2)
{
	size_t	len_joined;
	char	*joined;
	int		i;
	int		j;

	len_joined = ft_strlen(s1) + ft_strlen(s2) + 1;
	joined = malloc(sizeof(char) * len_joined);
	if (!joined)
		return (NULL);
