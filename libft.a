#include "libft.h"

void	ft_putnbr_fd(int n, int fd);

void	ft_putnbr_fd(int n, int fd)
{
	long long	number;

	number = n;
	if (number < 0)
	{
		write(fd, "-", 1);
		number *= -1;
	}
	while (number > 9)
	{
		ft_putnbr_fd(number / 10, fd);
		number %= 10;
	}
	number = number + 48;
	write(fd, &number, 1);
}

