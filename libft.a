#include "libft.h"

char    *ft_strtrim(char const *s1, char const *set);

char    *ft_strtrim(char const *s1, char const *set)
{
	size_t len;
	size_t len_trim;
	char * trim;
	size_t i;
	size_t j;

	len = ft_strlen(s1);
	i = 0;
	while(s1[i] && ft_strchr(set, s1[i]))
		i++;
	j = len - 1;
	while (j > i && ft_strchr(set, s1[j]))
		j--;
	len_trim = j - i;
	trim = malloc(sizeof(char) * (len_trim + 2));
	if (!trim)
		return (NULL);
	ft_memcpy(trim, s1 + i, len_trim + 1);
	trim[len_trim +1] = '\0';
	return (trim);
}

