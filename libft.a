#include "libft.h"

void	*ft_memchr(const void *s, int c, size_t n);

void	*ft_memchr(const void *s, int c, size_t n)
{
	while ((unsigned char *)s && n > 0)
	{
		if (*(unsigned char *)s == (unsigned char)c)
		{
			return ((unsigned char *)s);
		}
		s++;
		n--;
	}
	return (NULL);
}

